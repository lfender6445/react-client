{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "ordered-imports": false,
    "max-line-length": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "variable-name": false,
    "curly": false,
    "no-unused-variable": true
  },
  "rulesDirectory": []
}
